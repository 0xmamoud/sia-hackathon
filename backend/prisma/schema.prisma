datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model lease {
  id                 Int      @id @default(autoincrement())
  createdAt          DateTime @default(now())
  leaseName          String
  processId          String
  datasourceAudit    Json?
  datasourceExcel    Json?
  docResume          String?
  docAlert           Json?
}
